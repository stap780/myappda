
<div class="row">
<div class="col-6">
  <strong>Insid:</strong>
  <%= @variant.insid %>
</div>

<div class="col-6">
  <strong>Sku:</strong>
  <%= @variant.sku %>
</div>

<div class="col-6">
  <strong>Quantity:</strong>
  <%= @variant.quantity %>
</div>

<div class="col-6">
  <strong>Price:</strong>
  <%= @variant.price %>
</div>

<div class="col-6">
  <strong>Product:</strong>
  <%= @variant.product %>
</div>

</div>

<div class="row">
<div class="col-3 btn-group mt-5">
<%= link_to 'Edit', edit_variant_path(@variant) , class: 'btn btn-success' %> |
<%= link_to 'Back', variants_path, class: 'btn btn-outline-primary' %>
</div>
</div>
